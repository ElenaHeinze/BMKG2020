<SOURCE>
    <ID>geonames</ID>
    <ENDPOINT>C:/Users/Chris/Documents/GitHub/BMKG2020/output.nt</ENDPOINT>
    <VAR>?y</VAR>
    <PAGESIZE>-1</PAGESIZE>
    <RESTRICTION>?y rdf:type gn:Country</RESTRICTION>
    <PROPERTY>rdfs:label</PROPERTY>
    <TYPE>N-TRIPLE</TYPE>
</SOURCE>    
<TARGET>
    <ID>worldbank</ID>
    <ENDPOINT>C:/Users/Chris/Documents/GitHub/BMKG2020/output.nt</ENDPOINT>
    <VAR>?x</VAR>
    <PAGESIZE>-1</PAGESIZE>
    <RESTRICTION>?x rdf:type wb:GdpEntry</RESTRICTION>
