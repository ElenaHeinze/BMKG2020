<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE LIMES SYSTEM "limes.dtd">
<LIMES>
<PREFIX>
    <NAMESPACE>http://www.w3.org/1999/02/22-rdf-syntax-ns#</NAMESPACE>
    <LABEL>rdf</LABEL>
</PREFIX>
<PREFIX>
    <NAMESPACE>http://www.w3.org/2002/07/owl#</NAMESPACE>
    <LABEL>owl</LABEL>
</PREFIX>
<PREFIX>
    <NAMESPACE>http://dbpedia.org/ontology/</NAMESPACE>
    <LABEL>dbo</LABEL>
</PREFIX>
<PREFIX>
    <NAMESPACE>http://www.w3.org/2000/01/rdf-schema#</NAMESPACE>
    <LABEL>rdfs</LABEL>
</PREFIX>
<PREFIX>
    <NAMESPACE>http://www.w3.org/ns/r2rml#</NAMESPACE>
    <LABEL>rr</LABEL>
</PREFIX>
<PREFIX>
    <NAMESPACE>http://geonames.org/</NAMESPACE>
    <LABEL>gn</LABEL>
</PREFIX>
<PREFIX>
    <NAMESPACE>http://worldbank.org/</NAMESPACE>
    <LABEL>wb</LABEL>
</PREFIX>
<SOURCE>
    <ID>geonames</ID>
    <ENDPOINT>C:/Users/Elena/Desktop/Master/Year1/Courses/BMKG/Assignments/BMKG2020/output_geonames.nt</ENDPOINT>
    <VAR>?y</VAR>
    <PAGESIZE>-1</PAGESIZE>
    <RESTRICTION>?y rdf:type gn:Country</RESTRICTION>
    <PROPERTY>rdfs:label</PROPERTY>
    <TYPE>N-TRIPLE</TYPE>
</SOURCE>    
<TARGET>
    <ID>worldbank</ID>
    <ENDPOINT>C:/Users/Elena/Desktop/Master/Year1/Courses/BMKG/Assignments/BMKG2020/output_worldbank.nt</ENDPOINT>
    <VAR>?x</VAR>
    <PAGESIZE>-1</PAGESIZE>
    <RESTRICTION>?x rdf:type wb:GdpEntry</RESTRICTION>
    <PROPERTY>dbo:Country</PROPERTY>
	<TYPE>N-TRIPLE</TYPE>
</TARGET>
<METRIC>
    levenshtein(y.rdfs:label, x.rdfs:label)
</METRIC>
<ACCEPTANCE>
    <THRESHOLD>0.8</THRESHOLD>
    <FILE>accepted.nt</FILE>
    <RELATION>owl:sameAs</RELATION>
</ACCEPTANCE>
<REVIEW>
    <THRESHOLD>0.50</THRESHOLD>
    <FILE>reviewme.nt</FILE>
    <RELATION>owl:sameAs</RELATION>
</REVIEW>
<OUTPUT>N3</OUTPUT>
</LIMES>